{
  "name": "React-Boilerplate",
  "version": "1.0.0",
  "description": "A simple react boilerplate",
  "main": "index.js",
  "repository": "https://github.com/maxwashington/react-boilerplate.git",
  "author": "Max Washington",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build-server && yarn run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js",
    "test": "jest ./src"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.1.0"
  }
}
